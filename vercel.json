{
  "name": "vercel-typescript-express-api",
  "version": 2,
  "public": true,
  "builds": [
    {
      "command": "npm run build",
      "output": "dist",
      "src": "/src/app.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "backend/dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/app.ts"
    }
  ]
}